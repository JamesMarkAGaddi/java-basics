package org.acumen.training.codes.itema;

public class LotsOfErrors {

	public static void main(String[] args) {
		System.out.println("Hello, world!");
		message();
	}

	public static void message() {
		System.out.println("This program surely cannot");
		System.out.println("have any so-called \"errors\" in it");
	}
}
/* 
Description: This class converts temperature from F to C
*/
class Temperature {

	public static void main(String[] args) {
		double tempf = 98.6;
		double tempc = 0.0;
		tempc = convertTemp(tempf);
		System.out.println("Body temp in C is: " + tempc);
	}

	/*
	 * returns converted value
	 */
	public static double convertTemp(double tempf) {
		return (tempf - 32) * 5 / 9;
	}
}
